{
  "name": "finance-analyzer",
  "version": "1.0.0",
  "description": "Analyze your finance right now!",
  "main": "server.js",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "start": "node backend/Server.js",
    "client": "cd frontend && npm start",
