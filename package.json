    "database": "node start-server.js"
  },
  "author": "ESCLAPEZ Loïc, DIMECK Raphaël, VIDAL Odilon",
  "license": "MIT License",
  "dependencies": {
    "concurrently": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.7",
    "@babel/preset-env": "^7.26.7",
    "babel-jest": "^29.7.0"
  }
}
